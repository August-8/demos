/*js1.js*/

window.onload=function (){
	var oDiv1=document.getElementById('chose');
	var aBtn=oDiv1.getElementsByTagName('input');
	var aDiv=oDiv1.getElementsByTagName('div')[0];

	for(var i=0;i<aBtn.length;i++){
		aBtn[i].index=i;
		aBtn[i].onclick=function (){
			if(this.index=='0'){
				aBtn[1].className='btn2';
				this.className='btn1 active2';
				aDiv.innerHTML='<img src=img/svg1.png>';
			}
			if(this.index=='1'){
				aBtn[0].className='btn1';
				this.className='btn2 active2';
				aDiv.innerHTML='<img src=img/svg2.png>';
			}
		}
	}

    
	var oDiv4=document.getElementById('rtdiv');
	var abt=oDiv4.getElementsByTagName('input');
	var adivf=oDiv4.getElementsByTagName('div');
	for(var i=0;i<abt.length;i++){
		abt[i].index=i;
		abt[i].onclick=function(){
			for(var i=0;i<abt.length;i++){
				abt[i].className='';
				adivf[i].style.display='none';
			}
			this.className='active5';
			adivf[this.index].style.display='block';
			
		}
	}

}


function showor(){
	var s=document.getElementById("down-list");
	if(s.style.display=="block"){
		s.style.display="none";
	}else{
		s.style.display="block";
	}
}

function bigger(a){
	
	if (a=="c1") {
		var x=document.getElementById('circle1');
		x.className="circle-12";
	};
	if (a=="c2") {
		var x=document.getElementById('circle2');
		x.className="circle-22";
	};
	if (a=="c3") {
		var x=document.getElementById('circle3');
		x.className="circle-42";

	};
}
function smaller(a){
	if (a=="c1") {
		var x=document.getElementById('circle1');
		x.className="circle-1";
	};
	if (a=="c2") {
		var x=document.getElementById('circle2');
		x.className="circle-2";
	};
	if (a=="c3") {
		var x=document.getElementById('circle3');
		x.className="circle-4";

	};

}

